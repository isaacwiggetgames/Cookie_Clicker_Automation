javascript:(function(){var cI,bI,uI,gI,aI,aU,wI;function aCC(){const cR=400,cX=81.67,cY=409.51;cI=setInterval(function(){if(!Game.OnAscend){var a=Math.random()*Math.PI*2,r=Math.random()*cR;Game.mouseX=cX+r*Math.cos(a);Game.mouseY=cY+r*Math.sin(a);Game.ClickCookie();}else{clearInterval(cI);}},0.1);bI=setInterval(function(){if(!Game.OnAscend){var b=Game.ObjectsById,aB=b.filter(b=>b.price<=Game.cookies);if(aB.length>0){aB[Math.floor(Math.random()*aB.length)].buy();}}else{clearInterval(bI);}},1);var uu=[74,84,182,183,184,185,209,332,563,361,806,333,414];uI=setInterval(function(){if(!Game.OnAscend){var u=Game.UpgradesInStore.filter(u=>!u.bought&&u.canBuy()&&!uu.includes(u.id));if(u.length>0){u[Math.floor(Math.random()*u.length)].buy();}}},1);gI=setInterval(function(){if(!Game.OnAscend){Game.shimmers.filter(s=>s.type==='golden').forEach(s=>s.pop());}else{clearInterval(gI);}},1000);aI=setInterval(function(){if(!Game.OnAscend){var lb=document.getElementById('legacyButton');if(lb&&lb.style.display!=='none'){lb.click();setTimeout(function(){var ab=document.getElementById('promptOption0');if(ab){ab.click();}},1000);}}},86400000);aU=setInterval(function(){if(Game.OnAscend&&Game.AscendTimer===0){var au=Object.keys(Game.UpgradesById).filter(k=>{var u=Game.UpgradesById[k];return!u.bought&&u.pool=="prestige"&&u.basePrice<=Game.heavenlyChips;});if(au.length>0){Game.PurchaseHeavenlyUpgrade(au[Math.floor(Math.random()*au.length)]);}else{var ab=document.getElementById('ascendButton');if(ab&&ab.style.display!=='none'){ab.click();setTimeout(function(){var pb=document.getElementById('promptOption0');if(pb){pb.click();clearInterval(aU);}},1000);}}}},1000);wI=setInterval(function(){for(var i in Game.wrinklers){var w=Game.wrinklers[i];if(w.close&&w.phase>0){w.hp=0;}}},1);}function sA(){clearInterval(cI);clearInterval(bI);clearInterval(uI);clearInterval(gI);clearInterval(aI);clearInterval(aU);clearInterval(wI);}aCC();})();
